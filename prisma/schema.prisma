generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url  	    = env("DATABASE_URL")
}

model Employee {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  role      Role    
  hashed_password String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

enum Role {
  Owner
  Employee
}